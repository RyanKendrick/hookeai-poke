<!DOCTYPE html>
<html>
<head>
  <title>Past Orders</title>
</head>
<body>
  <h1>Past Orders</h1>
  <% order.forEach(function(item){ %>
    <p>Item Name: <%- item.name %>
    , Quantity: <%- item.quantity %>
    , Price: <%- item.price %>
    , Total Price: <%- item.quantity * item.price %>
    </p>
  <% }); %>
</body>
</html>
